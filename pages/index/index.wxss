page {
  background: #480495;
}

.top-bg {
  width: 100%;
}

.telecom {
  position: absolute;
  top: 20rpx;
  left: 25rpx;
  width: 25%;
}

.rule {
  position: absolute;
  top: 20rpx;
  right: 25rpx;
  color: #fff;
  font-size: 26rpx;
}

.draw {
  margin: -330rpx 7% 0;
  position: relative;
}

.draw > .box-bg {
  width: 100%;
  display: block;
}

.draw > .tip {
  position: absolute;
  left: -25rpx;
  top: 110rpx;
  font-size: 32rpx;
  color: #ff7a24;
  font-weight: bold;
  width: 25rpx;
  background: url("data:image/png;base64,") top/100% 100% no-repeat;
  padding: 20rpx;
}

.draw > .prize {
  position: absolute;
  top: 12%;
  width: 90%;
  left: 0;
  right: 0;
  margin: auto;
  display: flex;
  flex-wrap: wrap;
}

.draw > .prize > view {
  width: 33.3%;
  text-align: center;
  margin-bottom: 4%;
  position: relative;
}

.draw > .prize > view > image {
  width: 60%;
}

.draw > .prize > view > text {
  position: absolute;
  left: 50rpx;
  bottom: 15rpx;
  width: 50rpx;
  font-size: 23rpx;
}

.draw > .prize > view > .active {
  color: #fff;
}

.draw > .btn-bg {
  position: absolute;
  width: 100%;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  text-align: center;
}

.draw > .btn-bg > image {
  width: 50%;
}

.draw > .btn-bg > .open {
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  color: #fff;
  font-weight: bold;
  top: 15%;
  font-size: 40rpx;
  letter-spacing: 1px;
}

.prize-type {
  margin: 0 8%;
  background: url(data:image/png;base64,) top/100% 100% no-repeat;
}

.prize-type {
  padding: 40rpx;
  margin-bottom: 20rpx;
}

.prize-type > .tip {
  text-align: center;
  color: #fff;
  font-weight: bold;
  margin-bottom: 40rpx;
}

.prize-type > .type {
  display: flex;
  justify-content: space-around;
  font-size: 32rpx;
  margin-bottom: 20rpx;
}

.prize-type > .type > text {
  background: url(data:image/png;base64,) top/100% 100% no-repeat;
  padding: 20rpx 20rpx 15rpx;
  width: 25%;
  text-align: center;
  color: #8f00ff;
}

.prize-type > .type > text.active {
  background: url(data:image/png;base64,);
  color: #fff;
}

.prize-type > .items > image {
  width: 100%;
}
